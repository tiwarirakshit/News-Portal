                  <div class="col-md-6">
                    <div class="form-group">
                      <input
                        type="text"
                        class="form-control p-4"
                        name="name"
                        placeholder="Your Name"
                        required="required"
                      />
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <input
                        type="email"
                        class="form-control p-4"
                        name="email"
                        placeholder="Your Email"
                        required="required"
                      />
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <input
                    type="text"
                    class="form-control p-4"
                    placeholder="Subject"
                    name="subject"
                    required="required"
                  />
                </div>
                <div class="form-group">
                  <textarea
                    class="form-control"
                    rows="4"
                    name="message"
                    placeholder="Message"
                    required="required"
                  ></textarea>
                </div>
                <div>
                  <button
                    class="btn btn-primary font-weight-semi-bold px-4"
                    style="height: 50px"
                    type="submit"
                  >
                    Send Message
                  </button>
                </div>
